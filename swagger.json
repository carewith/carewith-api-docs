						"application/json;charset=utf-8": {
							"schema": {
								"$ref": "#/components/schemas/api-v1-patient-1612671555"
							},
							"examples": {
								"append-patient": {
									"value": "{\"name\":\"김태완\",\"birth\":\"2001-02-22\",\"since\":\"2019-03-02\",\"severity\":2}"
								}
							}
						}
					}
				},
